{
  "name": "@adexchange/aeg-ses",
  "version": "0.0.2",
  "description": "SES client",
  "main": "lib/ses.js",
  "author": "Ad Exchange Group",
  "repository": {
    "type": "git",
    "url": "https://github.com/AdExchangeGrp/aeg-ses.git"
  },
  "license": "UNLICENSED",
  "scripts": {
    "test": "mocha --recursive --compilers js:babel-core/register test/",
    "test:coverage": "istanbul cover _mocha -- --recursive --compilers js:babel-core/register test/",
    "eslint": "eslint .",
    "eslint:watch": "npm run eslint && onchange \"**/*.js\" \".eslint*\" -- npm run eslint",
    "flow": "flow; test $? -eq 0 -o $? -eq 2",
    "compile": "babel -d ./lib/ ./src/ && mkdir -p ./tmp && pax -wrs '/\\.js$/\\.js\\.flow/g' ./src ./tmp && rsync -a ./tmp/src/ ./lib/ && rm -Rf ./tmp ",
    "compile:watch": "npm run compile -- --watch",
    "dev": "npm run compile:watch",
    "build": "npm run eslint && npm run flow && npm run compile && istanbul cover _mocha -- --recursive --compilers js:babel-core/register --reporter mocha-junit-reporter test/unit",
    "patch": "git reset --hard HEAD && git clean -fd && npm run compile && npm version patch -m \"[ci skip] %s\" && git push origin master && git push origin master --tags && npm publish"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.21.0",
    "babel-eslint": "^7.1.1",
    "babel-plugin-transform-builtin-extend": "^1.1.0",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-stage-3": "^6.17.0",
    "eslint": "^3.13.1",
    "eslint-config-standard": "^6.2.1",
    "eslint-plugin-promise": "^3.4.0",
    "eslint-plugin-standard": "^2.0.1",
    "flow-bin": "^0.38.0",
    "istanbul": "^0.4.5",
    "mocha": "^3.2.0",
    "mocha-junit-reporter": "^1.9.1",
    "onchange": "^3.2.1",
    "should": "^11.1.2"
  },
  "dependencies": {
    "bluebird": "^3.4.7",
    "node-ses": "^1.1.0"
  },
  "engine": "node >= 4.1.0"
}
